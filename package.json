    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.js'"
    }
  },
  "lint": "eslint --ext .ts,.js,.vue ."
}

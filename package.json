{
  "name": "NuxtTemplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxt/babel-preset-app-edge": "^2.15.0-26817165.ac762299",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/pwa": "^3.0.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.5",
    "nuxt-property-decorator": "^2.8.8",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "autoprefixer": "^9.8.6",
    "breakpoint-sass": "^2.7.1",
    "cssnano": "^4.1.10",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.16.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "husky": "^4.3.6",
    "postcss-sort-media-queries": "^1.7.26",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "sort-css-media-queries": "^1.5.2",
    "vuex-module-decorators": "^1.0.1"
